<link rel="stylesheet" href="./css/homepage.css" />
{{#each newReleases as |newRelease|}}
<div class="column">
  <img src="{{newRelease.albumArtMedium}}" class="card-img-top" alt="..." />
  <div class="card-body">
    <p class="card-text">Album Name: {{newRelease.albumTitle}}</p>
    <p class="card-text">Artist Name: {{newRelease.artistName}}</p>
    <p class="card-text">Release Date: {{newRelease.releaseDate}}</p>
  </div>
</div>
{{/each}}